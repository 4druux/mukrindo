{
  "version": 2,
  "builds": [
    {
      "src": "api/run_clustering.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "main.py",
          "requirements.txt",
          "data_processing/**",
          "clustering/**",
          "database/**"
        ]
      }
    },
    {
      "src": "api/tes.py",
      "use": "@vercel/python"
    }
  ],
  "crons": [
    {
      "path": "/api/run_clustering",
      "schedule": "0 2 * * *"
    }
  ]
}
